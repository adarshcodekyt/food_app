



 <div class="col-sm-4 col-sm-offset-4">
<div class="text-center">
<h2>Sign up</h2>
</div>
<%= bootstrap_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">

    <%= f.email_field :email, autofocus: true, autocomplete: "email",:size=>"55",placeholder: "example@gmail.com"  %>
  </div></br>
  
 
  <div class="field">
  
    <%= f.text_field :first_name,:size=>"55", placeholder: "First name" %>
  </div></br>


  <div class="field">
    <%= f.text_field :last_name,:size=>"55", placeholder: "last name" %>
  </div></br>


  <div class="field">
    <%= f.text_field :phone,:size=>"55", placeholder: "phone"%>
  </div></br>



  <div class="field">
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password",:size=>"55", placeholder: "password"%>
  </div></br>

  <div class="field">
    <%= f.password_field :password_confirmation, autocomplete: "new-password",:size=>"55", placeholder: "password_confirmation"%>
  </div></br>

 <div class="actions">
    <%= f.submit "Sign up",class: 'btn btn-primary' %>
  </div>
<% end %>


<h5><strong><%= render "devise/shared/links" %></strong></h5>

</div>